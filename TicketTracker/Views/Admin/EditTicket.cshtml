@model Ticket
@{
    Layout = "_Layout";
}

<form asp-action="Edit" method="post">
    <div class="form-group">
        <label asp-for="TicketId"></label>
        <input class="form-control" asp-for="TicketId" readonly/>
    </div>
    <div class="form-group">
        <label asp-for="Opened"></label>
        <input class="form-control" asp-for="Opened" />
    </div>
    <div class="form-group">
        <label asp-for="Closed"></label>
        <input class="form-control" asp-for="Closed" />
    </div>
    <div class="form-group">
        <label asp-for="Title"></label>
        <input class="form-control" asp-for="Title"/>
    </div>
    <div class="form-group">
        <label asp-for="Description"></label>
        <input asp-for="Description" type="text" class="form-control"/>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-action="List" class="btn btn-danger">Cancel</a>
</form>
